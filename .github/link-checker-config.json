{
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "https://github.com/severin/awesome-synthetic-apps/blob/main/"
    }
  ],
  "ignorePatterns": [
    {
      "pattern": "^#"
    },
    {
      "pattern": "^mailto:"
    }
  ],
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [
    200, 201, 202, 203, 204, 205, 206, 207, 208, 226, 300, 301, 302, 303, 304,
    305, 306, 307, 308
  ],
  "timeout": "20s"
}
